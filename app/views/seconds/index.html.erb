<p id="notice"><%= notice %></p>

<div class="ui very padded container ">
  <h1>Seconds</h1>

  <table class="ui very basic celled table">
    <thead>
      <tr>
                  <th>Name</th>
                  <th>Normalized name</th>
                  <th>Second</th>
                  <th>Unixtime</th>
                  <th>Datetimesemanticzoomout</th>
                  <th>Datetimesemanticzoomin</th>
                <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @seconds.limit(45).each do |second| %>
        <tr>
                                    <td><%= second.name %></td>
                                                <td><%= second.normalized_name %></td>
                                                <td><%= second.second %></td>
                                                <td><%= second.unixtime %></td>
                                                <% @second.datetimeSemanticZoomOut.each do |datetimeSemanticZoomOut| -%>
                <%= link_to datetimeSemanticZoomOut.name, datetimeSemanticZoomOut %>
              <% end -%>
                                                <% @second.datetimeSemanticZoomIn.each do |datetimeSemanticZoomIn| -%>
                <%= link_to datetimeSemanticZoomIn.name, datetimeSemanticZoomIn %>
              <% end -%>
                                <td><%= link_to 'Show', second %></td>
          <td><%= link_to 'Edit', edit_second_path(second) %></td>
          <td><%= link_to 'Destroy', second, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Second', new_second_path %>
</div>
