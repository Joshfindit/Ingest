<p id="notice"><%= notice %></p>

<div class="ui">
  <a class="ui labeled icon button" href="<%= seconds_path %>">
    <i class="up arrow icon"></i>
    List Seconds
  </a>
  <a class="ui basic icon button" href="<%= edit_second_path(@second) %>">
    <i class="edit icon"></i>
    Edit
  </a>
</div>

<div class="ui hidden divider"></div>

<div class="ui vertical masthead center aligned segment">
  <h1 class="ui header">Second</h1>
</div>

<div class="ui text container">

  <p>
    <strong>Name:</strong>
    <%= @second.name -%>
  </p>

  <p>
    <strong>Second:</strong>
    <%= @second.second -%>
  </p>

  <p>
    <strong>Unixtime:</strong>
    <%= @second.unixtime -%>
  </p>

  <p>
    <strong>Datetimesemanticzoomout:</strong>
    <% if !@second.datetimeSemanticZoomOut.nil? -%>
      <% @second.datetimeSemanticZoomOut.each do |datetimeSemanticZoomOut| -%>
        <%= link_to datetimeSemanticZoomOut.name, datetimeSemanticZoomOut %>
      <% end -%>
    <% end -%>
  </p>

  <p>
    <strong>Datetimesemanticzoomin:</strong>
    <% if !@second.datetimeSemanticZoomIn.nil? -%>
      <% @second.datetimeSemanticZoomIn.each do |datetimeSemanticZoomIn| -%>
        <%= link_to datetimeSemanticZoomIn.name, datetimeSemanticZoomIn %>
      <% end -%>
    <% end -%>
  </p>
</div>
