<p id="notice"><%= notice %></p>

<div class="ui">
  <a class="ui labeled icon button" href="<%= years_path %>">
    <i class="up arrow icon"></i>
    List Years
  </a>
  <a class="ui basic icon button" href="<%= edit_year_path(@year) %>">
    <i class="edit icon"></i>
    Edit
  </a>
</div>

<div class="ui hidden divider"></div>

<div class="ui vertical masthead center aligned segment">
  <h1 class="ui header">Year</h1>
</div>

<div class="ui text container">

  <p>
    <strong>Name:</strong>
    <%= @year.name -%>
  </p>

  <p>
    <strong>Year:</strong>
    <%= @year.year -%>
  </p>

  <p>
    <strong>Datetimesemanticzoomout:</strong>
    <% if !@year.datetimeSemanticZoomOut.nil? -%>
      <% @year.datetimeSemanticZoomOut.each do |datetimeSemanticZoomOut| -%>
        <%= link_to datetimeSemanticZoomOut.name, datetimeSemanticZoomOut %>
      <% end -%>
    <% end -%>
  </p>

  <p>
    <strong>Datetimesemanticzoomin:</strong>
    <% if !@year.datetimeSemanticZoomIn.nil? -%>
      <% @year.datetimeSemanticZoomIn.each do |datetimeSemanticZoomIn| -%>
        <%= link_to datetimeSemanticZoomIn.name, datetimeSemanticZoomIn %>
      <% end -%>
    <% end -%>
  </p>
</div>
