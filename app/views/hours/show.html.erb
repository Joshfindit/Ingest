<p id="notice"><%= notice %></p>

<div class="ui text container">
  <h1 class="ui dividing header">Hour</h1>
</div>

<div class="ui text container">
      <p>
      <strong>Name:</strong>
              <%= @hour.name %>
          </p>
      <p>
      <strong>Normalized name:</strong>
              <%= @hour.normalized_name %>
          </p>
      <p>
      <strong>Hour:</strong>
              <%= @hour.hour %>
          </p>
      <p>
      <strong>Datetimesemanticzoomout:</strong>
              <% if @hour.datetimeSemanticZoomOut -%>
          <% @hour.datetimeSemanticZoomOut.each do |datetimeSemanticZoomOut| -%>
            <%= link_to datetimeSemanticZoomOut.name, datetimeSemanticZoomOut %>
          <% end -%>
        <% end -%>
          </p>
      <p>
      <strong>Datetimesemanticzoomin:</strong>
              <% if @hour.datetimeSemanticZoomIn -%>
          <% @hour.datetimeSemanticZoomIn.each do |datetimeSemanticZoomIn| -%>
            <%= link_to datetimeSemanticZoomIn.name, datetimeSemanticZoomIn %>
          <% end -%>
        <% end -%>
          </p>
  </div>

<%= link_to 'Edit', edit_hour_path(@hour) %> |
<%= link_to 'Back', hours_path %>
