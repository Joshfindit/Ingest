<p id="notice"><%= notice %></p>

<div class="ui">
  <a class="ui labeled icon button" href="<%= milliseconds_path %>">
    <i class="up arrow icon"></i>
    List Milliseconds
  </a>
  <a class="ui basic icon button" href="<%= edit_millisecond_path(@millisecond) %>">
    <i class="edit icon"></i>
    Edit
  </a>
</div>

<div class="ui hidden divider"></div>

<div class="ui vertical masthead center aligned segment">
  <h1 class="ui header">Millisecond</h1>
</div>

<div class="ui text container">

  <p>
    <strong>Name:</strong>
    <%= @millisecond.name -%>
  </p>

  <p>
    <strong>Millisecond:</strong>
    <%= @millisecond.millisecond -%>
  </p>

  <p>
    <strong>Datetimesemanticzoomout:</strong>
    <% if !@millisecond.datetimeSemanticZoomOut.nil? -%>
      <% @millisecond.datetimeSemanticZoomOut.each do |datetimeSemanticZoomOut| -%>
        <%= link_to datetimeSemanticZoomOut.name, datetimeSemanticZoomOut %>
      <% end -%>
    <% end -%>
  </p>
</div>
